#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long N;
    if (scanf("%lld", &N) != 1) return 0;

    // Geser dua kali ke kiri, lalu tiga kali ke kanan
    long long result = (N << 2) >> 3;

    printf("%lld\n", result);
    printf("EZBANGET\n");

    return 0;
